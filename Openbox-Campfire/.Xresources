! special
*.foreground:   #c5c8c6
*.background:   #222734
*.cursorColor:  #c5c8c6

! black
*.color0:       #282a2e
*.color8:       #838c9a

! red
*.color1:       #a53736
*.color9:       #a53736

! green
*.color2:       #f39863
*.color10:      #f39863

! yellow
*.color3:       #d85f42
*.color11:      #d85f42

! blue
*.color4:       #54607e
*.color12:      #54607e

! magenta
*.color5:       #54607e
*.color13:      #54607e

! cyan
*.color6:       #6a79a0
*.color14:      #6a79a0

! white
*.color7:       #707880
*.color15:      #f2d39a


! xterm
xterm*termName: xterm-256color
xterm*font: monospace
xterm*faceName: monospace:size=11
xterm*loginShell: true
xterm*vt100*geometry: 90x34
xterm*saveLines: 2000
xterm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
xterm*eightBitInput: false
xterm*modifyKeyboard: 2
xterm*selectToClipboard: true

! URxvt
URxvt*saveline: 15000
URxvt*termName: rxvt-256color
URxvt*iso14755: false
!URxvt*depth: 32
!URxvt*background: [95]#1C2023
URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*internalBorder: 0
URxvt*externalBorder: 0
URxvt*letterSpace: -1
URxvt.font: xft:monospace:size=10
URxvt.copyCommand: xclip -i -selection clipboard
URxvt.pasteCommand: xclip -o -selection clipboard
URxvt.urlLauncher: exo-open
URxvt.underlineURLs: true
URxvt.urlButton: 1
URxvt.geometry: 90x34
URxvt.tabbed.tabbar-fg: 4
URxvt.tabbed.tabbar-bg: 16
URxvt.tabbed.tab-fg: 15
URxvt.tabbed.tab-bg: 4
URxvt*buffered: false
URxvt.iconFile: /usr/share/icons/ZafiroCircle/apps/scalable/urxvt.svg
